import requests

informacoes = '{"Nome": "Mike", "Sobrenome": "Jorge", "Idade": "27"}'
requisicao = requests.patch(
    "https://teste-yt-445cb-default-rtdb.firebaseio.com/-O_CamhvFsimNrkqwhje.json",
    data=informacoes,
)
print(requisicao)
print(requisicao.json())  

# A REQUEST.PATCH vai alterar o ambiente selecionado dentro do banco de dados
# O dado que deseja alterar deve ser selecionado no link URL do banco de dados
